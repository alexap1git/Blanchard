    top: 5px;
    left: 0;
    width: 12px;
    height: 12px;
    text-align: center;
    vertical-align: 5px;
    border: 1px solid #999999;

}
.checke2 + span {
    display: flex;
    flex-direction: row;
    position: absolute;
    top: 4px;
    left: 0;
    width: 12px;
    height: 12px;
    text-align: center;
    vertical-align: 5px;
    border: 1px solid #999999;

}
.checke3 + span {
    display: flex;
    flex-direction: row;
    position: absolute;
    top: 4px;
    left: 0;
    width: 12px;
    height: 12px;
    text-align: center;
    vertical-align: 5px;
    border: 1px solid #999999;

}
.checke1 + span::before {
    content: url(../img/Ellipse16.svg);
    position: relative;
    top: -7px;
    left: 0px;
    right: 0px;
    bottom: 0px;
    width: 12px;
    height: 12px;
    opacity: 0;
    transition: opacity .3s ease-in-out;
}

.checke2 + span::before {
    content: url(../img/Ellipse16.svg);
    position: relative;
    top: -7px;
    left: 0px;
    right: 0px;
    bottom: 0px;
    width: 12px;
    height: 12px;
    opacity: 0;
    transition: opacity .3s ease-in-out;
}
.checke3 + span::before {
    content: url(../img/Ellipse16.svg);
    position: relative;
    top: -7px;
    left: 0px;
    right: 0px;
    bottom: 0px;
    width: 12px;
    height: 12px;
    opacity: 0;
    transition: opacity .3s ease-in-out;
}

.checke1:checked + span::before {
    opacity: 1;
}
.checke2:checked + span::before {
    opacity: 1;
}
.checke3:checked + span::before {
    opacity: 1;
}
.checke1:checked + span {
    border: 1px solid rgba(194, 131, 243, 1);
}
.checke1:checked ~ span {
    color: rgba(194, 131, 243, 1);
}
.checke1:hover + span {
    border: 1px solid rgba(194, 131, 243, 1);
    color: rgba(194, 131, 243, 1);
    transition: .6s;
}
.checke2:checked + span {
    border: 1px solid rgba(194, 131, 243, 1);
}
.checke2:checked ~ span {
    color: rgba(194, 131, 243, 1);
}
.checke2:hover + span {
    border: 1px solid rgba(194, 131, 243, 1);
    color: rgba(194, 131, 243, 1);
    transition: .6s;
}
.checke3:checked + span {
  border: 1px solid rgba(194, 131, 243, 1);
}
.checke3:checked ~ span {
  color: rgba(194, 131, 243, 1);
}
.checke3:hover + span {
    border: 1px solid rgba(194, 131, 243, 1);
    color: rgba(194, 131, 243, 1);
    transition: .6s;
}
