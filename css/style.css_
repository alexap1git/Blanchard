}
.event-link1::after,.event-link2::after,.event-link3::after,.event-link4::after,.event-link5::after {
    content: "";
    position: absolute;
    top: -8px;
    bottom: -8px;
    left: -8px;
    right: -8px;
    background-color: rgba(157, 92, 208, 1);
    z-index: -1;
    opacity: 0;
    transition: .3s;
}
.event-link1:focus,.event-link2:focus,.event-link3:focus,.event-link4:focus,.event-link5:focus {
    color: #ffffff;
    border-bottom: 1px solid #ffffff;
    transition: .3s;
}
.event-link1:focus::after,.event-link2:focus::after,.event-link3:focus::after,.event-link4:focus::after,.event-link5:focus::after {
    opacity: 1;
    transition: .3s;
}
