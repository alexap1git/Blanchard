  .mySwiper .swiper-slide:before {
    content: "";
    position: absolute;
    top: 0px;
    bottom: 0px;
    right: 0px;
    left: 0px;
    background: linear-gradient(0deg, rgba(194, 131, 243, 0.4), rgba(194, 131, 243, 0.4)), center no-repeat;
    z-index: -1;
    opacity: 0;
    transition-property: z-index, opacity;
    transition: .3s ease-in;
  }
  .mySwiper .swiper-slide:active:before {
    z-index: 1;
    opacity: 1;
  }

  .mySwiper .swiper-slide::after {
    content: "";
    position: absolute;
    top: 20px;
    bottom: 20px;
    right: 20px;
    left: 20px;
    background: linear-gradient(0deg, rgba(0, 0, 0, 0.5), rgba(0, 0, 0, 0.5)),url(../img/Group12.svg) center no-repeat;
    z-index: -1;
    opacity: 0;
    transition-property: z-index, opacity;
    transition: .3s ease-in;
  }
  .mySwiper .swiper-slide:hover::after {
    z-index: 1;
    opacity: 1;
  }
